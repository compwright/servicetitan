{
    "name": "compwright/servicetitan",
    "description": "ServiceTitan v2 API client for PHP",
    "keywords": [
        "servicetitan",
        "service-titan",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.org/compwright/servicetitan",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathon Hill",
            "homepage": "https://compwright.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "^3.8",
        "compwright/oauth2-servicetitan": "^1.0",
        "symfony/dotenv": "^7.2"
    },
    "suggest": {
        "compwright/oauth2-servicetitan": "ServiceTitan OAuth 2.0 Client Provider for The PHP League OAuth2-Client"
    },
    "autoload": {
        "psr-4": { "CompWright\\ServiceTitan\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "CompWright\\ServiceTitan\\Test\\" : "test/" }
    }
}
